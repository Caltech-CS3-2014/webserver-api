port 8080;

# The dispatch mechanism does substring matching on the
# specified locations, in order, and dispatches to the first
# one that matches.
# For example:
# * /static/foo matches /static/ and will dispatch to the first static
#   file handler
# * /foo will match "/" and will dispatch to the 404Handler.



handler StaticFileHandler {
  location /static/;
  document_root /foo/bar;
}

handler StaticFileHandler {
  location /static2/;
  document_root /a/b/c;
}

handler EchoHandler {
  location /echo;
}

handler Http404Handler {
  location /;
}
